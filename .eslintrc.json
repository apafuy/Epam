{
    "rules": {
      "quotes": ["error", "single"],
      "no-console": "off",
      "no-unused-vars": "warn"
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base", "prettier", "eslint:recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    }
}